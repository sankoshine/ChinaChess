import java.util.Scanner;

public class ChinaChess {

	private static int t1, t2;
	private static int f1, f2;
	private static Scanner in = new Scanner(System.in);

	public static void main(String[] args) {
		Board.init();
		Board.prit();
		while (true) {
			System.out.print("che: ");
			f1 = in.nextInt();
			f2 = in.nextInt();
			if (!Board.isChe(f1, f2)) {
				System.out.println("no chess here");
				continue;
			}
			System.out.println(Board.getName(f1, f2));
			System.out.print("cor: ");
			t1 = in.nextInt();
			t2 = in.nextInt();
			Board.setChe(f1, f2, t1, t2);
		}

	}
	
}