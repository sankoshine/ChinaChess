import java.io.IOException;

public class Lemon {

	public static void main(String[] args) throws IOException {
		String a,b,na,nb;
		int cnt;
		BasicFileIo fa = new BasicFileIo();
		BasicFileIo fb = new BasicFileIo();
		fa.openFile(BasicFileIo.INPUT, "a");
		fb.openFile(BasicFileIo.INPUT, "b");
		for(cnt=0;;cnt++){
			a = getCanonicalLine(fa.readString());
			b = getCanonicalLine(fb.readString());
			if(a.equals(b))continue;
			System.out.println('#' + cnt +':');
			System.out.println(a);
			System.out.println(b);
			System.out.println("");
		}
		
	}
	
	public static String getCanonicalLine (String str){
		String ans_str = "",s;
		s = str.trim();
		int i,j=0;
		boolean f=true;
		for(i=0;i<s.length();i++){
			if(!f && s.charAt(i)==' ')continue;
			if(f && s.charAt(i)==' ' || !f && s.charAt(i)!=' '){
				f=!f;
			}
			ans_str += s.charAt(i);
		}
		return ans_str.toLowerCase();
	}	

}
